{
  "name": "termio",
  "version": "1.0.0",
  "description": "Perfectly convert ANSI escape codes into HTML.",
  "main": "index.js",
  "bin": {
    "termio": "./bin/termio"
  },
  "scripts": {
    "build": "pkg-clean && rollup -c",
    "prepublish": "npm run build",
    "test": "pkg-test",
    "flow": "pkg-flow",
    "coverage": "pkg-coverage",
    "lint": "pkg-lint"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/stayradiated/termio.git"
  },
  "keywords": [
    "escape",
    "ansi",
    "html",
    "convert",
    "bcat"
  ],
  "author": "George Czabania",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stayradiated/termio/issues"
  },
  "homepage": "https://github.com/stayradiated/termio",
  "dependencies": {
    "colr": "^1.2.2",
    "commander": "^2.15.1",
    "escape-html": "^1.0.3",
    "fast-equals": "^1.2.1",
    "splinter": "1.0.0",
    "through": "^2.3.8",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@mishguru/package": "^2.2.2",
    "mocha": "^5.1.0",
    "rollup": "^0.57.1"
  },
  "babel": {
    "presets": [
      "flow",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
